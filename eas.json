{
  "cli": {
    "version": ">= 12.6.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_BASE_URL_LOCAL": "http://localhost:3000"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_BASE_URL_PROD": "https://runmate-back-api.onrender.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_BASE_URL_PROD": "https://runmate-back-api.onrender.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
