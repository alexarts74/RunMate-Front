{
  "cli": {
    "version": ">= 12.6.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "API_BASE_URL_PROD": "http://localhost:3000"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "API_BASE_URL_PROD": "https://runmate-back-api.onrender.com"
      },
      "node": "20",
      "install": {
        "customCommand": "npm ci --legacy-peer-deps"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "API_BASE_URL_PROD": "https://runmate-back-api.onrender.com"
      },
      "node": "20",
      "install": {
        "customCommand": "npm ci --legacy-peer-deps"
      }
    }
  },
  "submit": {
    "preview": {},
    "production": {}
  }
}
